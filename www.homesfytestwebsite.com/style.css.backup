/* Map Section Styles */
.mapSection {
    background: linear-gradient(135deg, #0a1828 0%, #152838 100%);
    padding: 80px 0;
    color: white;
}

.map-container {
    max-width: 1200px;
    margin: 0 auto;
    padding: 0 40px;
}

.map-title {
    font-size: 48px;
    font-weight: 700;
    text-align: center;
    margin-bottom: 60px;
    background: linear-gradient(135deg, #e5b973 0%, #e5b973 50%, #e5b973 100%);
    -webkit-background-clip: text;
    -webkit-text-fill-color: transparent;
    background-clip: text;
    position: relative;
}


.map-content {
    display: grid;
    grid-template-columns: 1fr 1fr;
    gap: 40px;
    align-items: start;
}

.map-left {
