.galleryPrev,
.galleryNext {
    position: absolute;
    top: 50%;
    transform: translateY(-50%);
    background: rgba(229, 185, 115, 0.9);
    border: none;
    color: #0a1828;
    width: 50px;
    height: 50px;
    border-radius: 50%;
    cursor: pointer;
    display: flex;
    align-items: center;
    justify-content: center;
    font-size: 20px;
    transition: all 0.3s ease;
    box-shadow: 0 4px 15px rgba(229, 185, 115, 0.3);
