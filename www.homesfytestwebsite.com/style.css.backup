    position: relative;
    width: 100%;
    height: 450px;
    border-radius: 12px;
    border: 2px solid rgba(229, 185, 115, 0.3);
    box-shadow: 0 8px 32px rgba(0, 0, 0, 0.3);
    overflow: hidden;
}

